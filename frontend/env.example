# TOE AI Frontend Environment Configuration
# Copy this file to .env and update with your actual values

# ================================================
# API CONFIGURATION
# ================================================
# Development
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Production (Update with your domain)
# VITE_API_BASE_URL=https://yourdomain.com/api/v1

# ================================================
# STRIPE CONFIGURATION
# ================================================
# Test Mode (Development)
# VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_publishable_key_here

# Live Mode (Production) - UNCOMMENT FOR PRODUCTION
# VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_live_publishable_key_here

# ================================================
# SUPABASE CONFIGURATION
# ================================================
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ================================================
# GOOGLE OAUTH
# ================================================
# Development
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Production (Update with your domain)
# VITE_GOOGLE_CLIENT_ID=your_production_google_client_id_here

# ================================================
# APPLICATION SETTINGS
# ================================================
VITE_APP_NAME=TOE AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# ================================================
# FEATURE FLAGS
# ================================================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
